        "github": {
            "url": "https://github.com/9527q/diagram-tcpip-note"
        },
        "donate": {
            "wechat": "http://ww1.sinaimg.cn/large/0062WsWaly1gfomav7w3qj30iw0ir0uq.jpg",
            "alipay": "http://ww1.sinaimg.cn/large/0062WsWaly1gfombfqs7jj30f70fdtae.jpg",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "isShowTocTitleIcon": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right",
