        "hide-element",
        "back-to-top-button",
        "chapter-fold",
        "splitter",
        "-lunr",
        "-search",
        "search-pro",
        "insert-logo",
        "tbfed-pagefooter",
        "popup",
        "github",
        "anchors",
        "donate",
